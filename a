[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void button1_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            string textoValorSaque = valorOperacao.Text;[m
[32m+[m
[32m+[m[32m            double valorDeposito = Convert.ToDouble(textoValorSaque);[m
[32m+[m
[32m+[m[32m            int indiceSelecionado = comboContas.SelectedIndex;[m
[32m+[m
[32m+[m[32m            Conta contaSelecionada = this.contas[indiceSelecionado];[m
[32m+[m[32m            contaSelecionada.Deposita(valorDeposito);[m
[32m+[m
[32m+[m[32m            this.MostraConta(contaSelecionada);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void button2_Click(object sender, EventArgs eA)[m
[32m+[m[32m        {[m
[32m+[m[32m            string textoValorSaque = valorOperacao.Text;[m
[32m+[m
[32m+[m[32m            double valorSaque = Convert.ToDouble(textoValorSaque);[m
[32m+[m[32m            Conta contaSelecionada = this.BuscaContaSelecionada();[m
[32m+[m
[32m+[m[32m            try {[m[41m [m
[32m+[m[32m                contaSelecionada.Saca(valorSaque);[m
[32m+[m[32m                MessageBox.Show("Dinheiro Liberado");[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (SaldoInsuficienteException e)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show("Saldo insuficiente. " + e.Message);[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (ArgumentException e)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show("NÃ£o Ã© possÃ­vel sacar um valor negativo. " + e.Message);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.MostraConta(contaSelecionada);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void MostraConta(Conta conta)[m
[32m+[m[32m        {[m
[32m+[m[32m            textoTitular.Text = conta.Titular.Nome;[m
[32m+[m[32m            textoSaldo.Text = Convert.ToString(conta.Saldo);[m
[32m+[m[32m            textoNumero.Text = Convert.ToString(conta.Numero);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private Conta BuscaContaSelecionada()[m
[32m+[m[32m        {[m
[32m+[m[32m            int indiceSelecionado = comboContas.SelectedIndex;[m
[32m+[m[32m            return this.contas[indiceSelecionado];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        private void comboBox1_SelectedIndexChanged(object sender, EventArgs e)[m
