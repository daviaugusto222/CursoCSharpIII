[32m+[m[32mï»¿using Caelum.CaixaEletronico.Modelo.Usuarios;[m
[32m+[m[32musing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace Caelum.CaixaEletronico.Modelo.Contas[m
[32m+[m[32m{[m
[32m+[m[32m    abstract class Conta[m
[32m+[m[32m    {[m
[32m+[m[32m        public int Numero { get; set; }[m
[32m+[m[32m        public Cliente Titular { get; set; }[m
[32m+[m[32m        public double Saldo { get; protected set; }[m
[32m+[m
[32m+[m[32m        public virtual void Deposita(double valor)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (valor > 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                this.Saldo += valor;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public abstract void Saca(double valor);[m
[32m+[m
[32m+[m[32m        public void TransferePara( Conta destino, double valor)[m
[32m+[m[32m        {[m
[32m+[m[32m            this.Saca(valor);[m
[32m+[m[32m            destino.Deposita(valor);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public double CalculaRendimentoAnual()[m
[32m+[m[32m        {[m
[32m+[m[32m            double saldoNaqueleMes = this.Saldo;[m
[32m+[m
[32m+[m[32m            for (int i = 0; i < 12; i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                saldoNaqueleMes = saldoNaqueleMes * 1.007;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            double rendimento = saldoNaqueleMes - this.Saldo;[m
[32m+[m
[32m+[m[32m            return rendimento;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public override bool Equals(object obj)[m
[32m+[m[32m        {[m
[32m+[m[32m            if(obj != null) {[m
[32m+[m[32m                Conta outra = (Conta)obj;[m
[32m+[m
[32m+[m[32m                bool numero = (this.Numero == outra.Numero);[m
[32m+[m[32m                bool titular = (this.Titular.Equals(outra.Titular));[m
[32m+[m
[32m+[m[32m                return numero && titular;[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m            {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public override string ToString()[m
[32m+[m[32m        {[m
[32m+[m[32m            return "Numero: " + this.Numero;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
