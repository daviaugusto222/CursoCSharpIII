[32m+[m[32m            Conta contaDoGuilherme = new ContaPoupanca();[m
[32m+[m[32m            contaDoGuilherme.Titular = new Cliente();[m
[32m+[m[32m            contaDoGuilherme.Titular.Nome = "Guilherme";[m
[32m+[m[32m            contaDoGuilherme.Numero = 2;[m
[32m+[m[32m            contas[1] = contaDoGuilherme;[m
[32m+[m
[32m+[m[32m            Conta contaDoMauricio = new ContaInvestimento();[m
[32m+[m[32m            contaDoMauricio.Titular = new Cliente();[m
[32m+[m[32m            contaDoMauricio.Titular.Nome = "Mauricio";[m
