[32m+[m[32m   class ContaCorrente : Conta, ITributavel[m
[32m+[m[32m    {[m
[32m+[m[32m        private static int totalDeContas = 0;[m
[32m+[m
[32m+[m[32m        public ContaCorrente()[m
[32m+[m[32m        {[m
[32m+[m[32m                ContaCorrente.totalDeContas++;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public int ProximaConta()[m
[32m+[m[32m        {[m
[32m+[m[32m            return ContaCorrente.totalDeContas + 1;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public double CalculaTributo()[m
[32m+[m[32m        {[m
[32m+[m[32m            return this.Saldo * 0.02;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public override void Saca(double valor)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (valor > this.Saldo )[m
[32m+[m[32m            {[m
[32m+[m[32m                throw new SaldoInsuficienteException("Valor do saque maior que o saldo");[m
[32m+[m[32m            } else if(valor < 0) {[m
[32m+[m[32m                throw new ArgumentException();[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m            {[m
[32m+[m[32m                this.Saldo -= (valor + 0.1);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
