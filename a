[32m+[m[32mï»¿using System;[m
[32m+[m
[32m+[m[32mnamespace Caelum.CaixaEletronico.Modelo.Usuarios[m
[32m+[m[32m{[m
[32m+[m[32m        class Cliente[m
[32m+[m[32m        {[m
[32m+[m[32m            public string Nome { get; set; }[m
[32m+[m[32m            public string Rg { get; set; }[m
[32m+[m[32m            public string cpf;[m
[32m+[m[32m            public string endereco;[m
[32m+[m[32m            public int idade;[m
[32m+[m[32m            private bool EEmancipado;[m
[32m+[m
[32m+[m[32m            public Cliente(string nome)[m
[32m+[m[32m            {[m
[32m+[m[32m                this.Nome = nome;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public Cliente() { }[m
[32m+[m
[32m+[m[32m            public bool PodeAbrirContaSozinho[m
[32m+[m[32m            {[m
[32m+[m[32m                get[m
[32m+[m[32m                {[m
[32m+[m[32m                    var maiorDeIdade = this.idade >= 18;[m
[32m+[m[32m                    var emancipado = (this.EEmancipado);[m
[32m+[m[32m                    var possuiCPF = !string.IsNullOrEmpty(this.cpf);[m
[32m+[m[32m                    return (maiorDeIdade || emancipado) && possuiCPF;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public bool EhMaiorDeIdade()[m
[32m+[m[32m            {[m
[32m+[m[32m                return this.idade >= 18;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public override bool Equals(Object objeto)[m
[32m+[m[32m            {[m
[32m+[m[32m                Cliente outroCliente = (Cliente)objeto;[m
[32m+[m[32m                return this.Rg == outroCliente.Rg;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public override string ToString()[m
[32m+[m[32m                {[m
[32m+[m[32m                    return "Nome: " + this.Nome + " RG: " + this.Rg;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m}[m
