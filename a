[32m+[m[41m        [m
[32m+[m[32m            contaSelecionada.TransferePara(contaDestino, valorTransferencia);[m
[32m+[m
[32m+[m[32m            this.MostraConta(contaSelecionada);[m
[32m+[m
[32m+[m[32m        }[m
